<% if form_object.errors.any? %>
  <div class="row">
    <div class="alert alert-error span8">
      <h5>There were some errors</h5>
      <ul>
      <% form_object.errors.full_messages.each do |error_message| %>
        <li><%= error_message %></li>
      <% end %>
      </ul>
    </div>
  </div>
<% end %>
